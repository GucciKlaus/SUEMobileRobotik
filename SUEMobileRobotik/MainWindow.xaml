<Window x:Class="SUEMobileRobotik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SUEMobileRobotik" xmlns:UserControlRobotik="clr-namespace:UserControlRobotik;assembly=UserControlRobotik"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="2*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <UserControlRobotik:UserControl1 UC_Description="Left wheel"></UserControlRobotik:UserControl1>
        <Image Grid.Row="0" Grid.Column="1" Source="/MobRobotDraufsicht.png"></Image>
        <UserControlRobotik:UserControl1 Grid.Row="0" Grid.Column="3" UC_Description="Right wheel"></UserControlRobotik:UserControl1>
    </Grid>
</Window>
